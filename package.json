{
  "name": "vas-assets",
  "version": "0.0.0",
  "description": "asset handler and compiler for vas-http",
  "main": "index.js",
  "scripts": {
    "start": "node-dev example/server",
    "test:deps": "dependency-check . && dependency-check . --extra --no-dev -i es2040",
    "test:lint": "standard",
    "test:node": "NODE_ENV=test run-default tape test/*.js --",
    "test:coverage": "NODE_ENV=test nyc npm run test:node",
    "test:coverage:report": "nyc report --reporter=lcov npm run test:node",
    "test": "npm-run-all -s test:node test:lint test:deps"
  },
  "browserify": {
    "transform": [
      "es2040"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/vas-assets.git"
  },
  "keywords": [],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/vas-assets/issues"
  },
  "homepage": "https://github.com/ahdinosaur/vas-assets#readme",
  "devDependencies": {
    "catstack-log": "github:catstack/catstack-log",
    "dependency-check": "^2.7.0",
    "depject": "^3.1.5",
    "node-dev": "^3.1.3",
    "npm-run-all": "^4.0.1",
    "nyc": "^10.1.2",
    "run-default": "^1.0.0",
    "standard": "^8.6.0",
    "tape": "^4.6.3",
    "vas-http": "github:ahdinosaur/vas-http"
  },
  "dependencies": {
    "accepts": "^1.3.3",
    "bankai": "^5.2.1",
    "es2040": "^1.2.3"
  }
}
